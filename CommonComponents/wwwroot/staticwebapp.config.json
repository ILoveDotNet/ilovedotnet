{
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": [
      "/*.{css,gif,ico,jpg,js,png,txt,svg,woff,woff2,ttf,map,json,br,gz,html,md,eot,otf,dll,blat,wasm,dat,webp,xml}",
      "/css/*",
      "/appicons/*",
      "/image/*",
      "/js/*"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  }
}